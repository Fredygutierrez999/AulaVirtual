<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    
    
    <navigation-rule id="1">
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule id="2">
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule id="3">
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consultaUsuarios</from-outcome>
            <to-view-id>/pages/usuario/consultaUsuarios.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    <application>
        <locale-config>
            <default-locale>es</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>mensajes</base-name>
            <var>msgs</var>
        </resource-bundle>
        <message-bundle>jsf</message-bundle>
    </application>
        

</faces-config>